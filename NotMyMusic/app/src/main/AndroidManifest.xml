<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nathan.hardy.notmymusic" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.nathan.hardy.notmymusic.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nathan.hardy.notmymusic.SearchActivity"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name="com.nathan.hardy.notmymusic.AddActivity"
            android:label="@string/title_activity_add" >
        </activity>
        <activity
            android:name="com.nathan.hardy.notmymusic.DetailActivity"
            android:label="@string/title_activity_detail" >
        </activity>
    </application>

</manifest>
